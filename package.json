    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "sequelize-cli": "^6.2.0",
    "sequelize": "^6.3.5",
    "uuidv4": "^6.0.7"
  },
