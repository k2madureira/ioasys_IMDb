  "scripts": {
    "db:migrate": "sequelize db:migrate",
    "build": "babel src --extensions \" .js,.ts\" --out-dir dist --copy-files",
    "start": "node dist/shared/server.js",
    "start:ts": "ts-node -r tsconfig-paths/register src/shared/server.ts",
    "dev:server": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules src/shared/server.ts",
    "test": "jest --no-cache",
    "test:clean": "jest --clearCache",
    "test:user": "jest src/modules/User/__tests__/User.spec.ts NODE_ENV=test",
    "test:movie": "jest src/modules/Movie/__tests__/Movie.spec.ts NODE_ENV=test"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "bcrypt": "^5.0.0",
