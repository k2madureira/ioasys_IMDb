{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
      "outDir": "./dist",
      "rootDir": "./src",
    "strict": true,
    "strictPropertyInitialization": false,
    "baseUrl": "./src",
    "allowJs": true,
    "paths": {
      "@config/*": ["config/*"],
      "@modules/*": ["modules/*"],
      "@shared/*": ["shared/*"]
    },
    "esModuleInterop": true,
     "experimentalDecorators": true,
     "emitDecoratorMetadata": true,
    "forceConsistentCasingInFileNames": true
  }
}